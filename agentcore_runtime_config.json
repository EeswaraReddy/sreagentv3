{
    "runtime": {
        "handler": "agents.main:handler",
        "timeout_seconds": 300,
        "memory_mb": 1024,
        "python_version": "3.11",
        "framework": "strands-agents"
    },
    "environment": {
        "RCA_BUCKET": "${RCA_BUCKET_NAME}",
        "RCA_PREFIX": "rca/",
        "GATEWAY_ENDPOINT": "${GATEWAY_ENDPOINT_URL}",
        "BEDROCK_MODEL_ID": "us.anthropic.claude-sonnet-4-20250514",
        "LOG_LEVEL": "INFO",
        "METRICS_NAMESPACE": "IncidentHandler"
    },
    "bedrock": {
        "model_access": [
            "anthropic.claude-sonnet-4-20250514",
            "anthropic.claude-3-haiku-20240307",
            "amazon.titan-embed-text-v2:0"
        ],
        "inference_profile": "default"
    },
    "agentcore": {
        "gateway": {
            "endpoint": "${GATEWAY_ENDPOINT_URL}",
            "authentication": "cognito_jwt",
            "semantic_search": true,
            "search_header": "x-amz-bedrock-agentcore-search"
        },
        "memory": {
            "enabled": false,
            "type": "none"
        },
        "observability": {
            "enabled": true,
            "xray_tracing": true,
            "cloudwatch_metrics": true,
            "log_level": "INFO"
        }
    },
    "scaling": {
        "min_instances": 0,
        "max_instances": 10,
        "target_utilization": 70
    }
}